  // - - - PythonPath settings- - - - - - - - - - - - - 
  "python.defaultInterpreterPath": "./venv/bin/python",
  // - - - Formatter settings - - - - - - - - - - - - -
  "python.formatting.provider": "black",
  // - - - TodoHighlighter settings - - - - - - - - - - 
  "todoHighlighter.exclude": [
    "**/node_modules/**",
    "**/dist/**",
    "**/venv/**",
  ],
  // - - - liveSassCompile settings - - - - - - - - - - 
  "liveSassCompile.settings.formats": [
    {
      "format": "compressed",
      "extensionName": ".min.css",
      "savePath": "/dashboard/dashboard_dev/src/styles/css"
