{
  // - - - StatusBar settings - - - - - - - - - - - - -
  "workbench.colorCustomizations": {
    "statusBar.background": "#666666",
  },
  // - - - Folder's icon settings - - - - - - - - - - -
  "material-icon-theme.folders.associations": {
    "dashboard": "Content",
    "distributor": "App",
    "eds_service": "Meta",
    "venv": "Python",
    "sh": "Scripts",
    "logger": "Log",
    "managers": "Quasar",
